{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTORCH_ENABLE_MPS_FALLBACK": "1",
                "WANDB_MODE": "offline"
            },
            "pythonPath": "${workspaceFolder}/liplad-with-yolov8/bin/python"  // Adjust the path if necessary

        }
    ]
}